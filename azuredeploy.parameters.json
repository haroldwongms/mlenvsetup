{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/haroldwongms/mlenvsetup/master"
		},
		"location": {
			"value": "[resourceGroup().location]"
		},
		"virtualMachineName": {
			"value": "dsvm"
		},
		"virtualMachineSize": {
			"value": "Standard_DS3_v2"
		},
		"adminUsername": {
			"value": "adminuser"
		},
		"adminPassword": {
			"value": "GEN-UNIQUE-PASSWORD"
		},
		"appName": {
			"value": "myfunctionapp"
		}
    }
}